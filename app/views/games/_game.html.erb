<div id="<%= dom_id game %>">
  <p>
    <strong>Title:</strong>
    <%= game.title %>
  </p>

  <p>
    <strong>Console type:</strong>
    <%= game.console_type %>
  </p>

  <h2>Accounts that own this game:</h2>
  <ul>
    <% game.accounts.each do |account| %>
      <li><%= account.email %></li>
    <% end %>
  </ul>

</div>
